{
    "material-icon-theme.files.associations": {
      "settings.json": "Raml",
      "config.json": "Raml",
      "*.lang.json": "i18n",
      "lang.json": "i18n",
      "gitignore": "Git",
      "accounts.txt": "settings",
      "Source2Viewer-CLI": "settings",
      "libTinyEXR.Native.so": "settings",
      "libSkiaSharp.so": "settings",
    },
    "material-icon-theme.folders.theme": "specific",
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
      ".env": ".env.development, .env.production, .env.test, .env.example",
      "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, tsconfig.json, .eslintrc.json, .gitignore, LICENSE",
      "accounts.txt": "libSkiaSharp.so, libTinyEXR.Native.so, Source2Viewer-CLI"
    },
    "typescript.suggest.autoImports": true,
    "window.title": "${rootName}${separator}${appName}",
    "files.autoSave": "afterDelay",
  }